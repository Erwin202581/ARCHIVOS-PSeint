Proceso Promedio_de_tres_notas
	Definir nombre_Estudiante Como Caracter;
	definir nota_1, nota_2, nota_3 Como Entero;
	escribir "Nombre del Alumno";
	Leer nombre_Estudiante;
	Escribir "Escriba la primera nota";
	Leer nota_1;
	Escribir "Escriba la segunda nota";
	Leer nota_2;
	Escribir "Escriba la tercera nota";
	Leer nota_3;
	Escribir nombre_Estudiante;
	Escribir "Su nota final quedó en: ", (nota_1 + nota_2 + nota_3) / 3;
	
FinProceso
